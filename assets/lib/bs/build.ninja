bs_package_flags = -bs-package-name ex_reason
src_root_dir = /home/henry/rd/ex_reason/assets
bsc = /home/henry/rd/ex_reason/assets/node_modules/bs-platform/lib/bsc.exe
bsdep = /home/henry/rd/ex_reason/assets/node_modules/bs-platform/lib/bsb_helper.exe
warnings = -w -30-40+6+7+27+32..39+44+45+101
bsc_flags = -bs-suffix -nostdlib -I '/home/henry/rd/ex_reason/assets/node_modules/bs-platform/lib/ocaml' -bs-no-version-header -bs-super-errors -color always
ppx_flags = 
bs_package_includes = -I /home/henry/rd/ex_reason/assets/node_modules/reason-react/lib/ocaml
bs_package_dev_includes = 
namespace = 
bsb_dir_group = 0
refmt = /home/henry/rd/ex_reason/assets/node_modules/bs-platform/lib/refmt.exe
reason_react_jsx = -ppx '/home/henry/rd/ex_reason/assets/node_modules/bs-platform/lib/reactjs_jsx_ppx_2.exe'
refmt_flags = --print binary
bsc_lib_includes = -I js
rule build_ast_and_module_sets_from_re
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx}  ${ppx_flags} ${warnings} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast -impl ${in}
  description = [34mBuilding[39m [2m${out}[22m
build js/page.mlast : build_ast_and_module_sets_from_re $src_root_dir/js/page.re
rule build_deps
  command = ${bsdep} ${namespace} -g ${bsb_dir_group} -MD ${in}
  description = [34mBuilding[39m [2m${out}[22m
build js/page.mlast.d : build_deps js/page.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${warnings} ${bsc_flags} -o ${out} -c  ${in} $postbuild
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build js/page.cmj |  $src_root_dir/js/page.bs.js js/page.cmi : build_cmj_cmi js/page.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:js
build js/root.mlast : build_ast_and_module_sets_from_re $src_root_dir/js/root.re
build js/root.mlast.d : build_deps js/root.mlast
build js/root.cmj |  $src_root_dir/js/root.bs.js js/root.cmi : build_cmj_cmi js/root.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:js
build build.ninja : phony  ||  js/root.mlast.d js/page.mlast.d
